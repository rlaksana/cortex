# CI/CD Environment Configuration - QDRANT ONLY
# PostgreSQL is NO LONGER SUPPORTED

# OpenAI API key for CI/CD (must be provided by CI secrets)
OPENAI_API_KEY=${OPENAI_API_KEY}

# Qdrant configuration for CI/CD
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=cortex-ci-test
QDRANT_TIMEOUT=30000

# Vector embedding configuration for CI/CD
VECTOR_SIZE=1536  # 1536=text-embedding-3-small, 3072=*-3-large
VECTOR_DISTANCE=Cosine
EMBEDDING_MODEL=text-embedding-3-small
EMBEDDING_BATCH_SIZE=10

# Search configuration for CI/CD
SEARCH_LIMIT=50
SEARCH_MODE=auto
ENABLE_CACHE=true
CACHE_TTL=3600
SIMILARITY_THRESHOLD=0.7

# Basic settings
LOG_LEVEL=info
NODE_ENV=test

# Performance settings for CI/CD
DB_MAX_CONNECTIONS=10
DB_CONNECTION_TIMEOUT=30000
API_TIMEOUT=30000
RETRY_ATTEMPTS=3

# CI-specific settings
CI=true
COVERAGE_ENABLED=true
COVERAGE_DIR=coverage
COVERAGE_REPORTS_DIR=coverage/reports

# Test scope for CI/CD
CORTEX_ORG=ci-org
CORTEX_PROJECT=ci-project
CORTEX_BRANCH=main

# MCP settings for CI/CD
MCP_SERVER_NAME=cortex-ci
MCP_SERVER_VERSION=2.0.0-ci
MCP_TRANSPORT=stdio
MCP_MAX_BATCH_SIZE=100
