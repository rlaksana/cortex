# Simple Test Configuration - QDRANT ONLY
# PostgreSQL is NO LONGER SUPPORTED

# OpenAI API key for testing
OPENAI_API_KEY=test-key-for-testing-only

# Qdrant configuration for simple testing
QDRANT_URL=http://localhost:6333
QDRANT_COLLECTION_NAME=cortex-simple-test
QDRANT_TIMEOUT=30000

# Vector settings for testing
VECTOR_SIZE=1536  # 1536=text-embedding-3-small, 3072=*-3-large
EMBEDDING_MODEL=text-embedding-3-small

LOG_LEVEL=debug
NODE_ENV=test
