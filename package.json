{
  "name": "cortex-memory-mcp",
  "version": "2.0.0",
  "description": "Cortex Memory MCP Server - Advanced knowledge management with Qdrant vector database support",
  "main": "dist/silent-mcp-entry.js",
  "type": "module",
  "bin": {
    "cortex": "dist/silent-mcp-entry.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "start:raw": "node dist/index.js",
    "dev": "node dist/index.js",
    "build": "tsc && node scripts/fix-imports.mjs",
    "build:index": "tsc src/index.ts --outDir dist --target es2022 --module esnext --moduleResolution node --esModuleInterop --allowSyntheticDefaultImports --skipLibCheck",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "lint:quiet": "eslint src --quiet",
    "lint:hard": "eslint src tests --ext .ts --max-warnings=500",
    "type-check": "tsc --noEmit",
    "type-check:index": "tsc src/index.ts --noEmit",
    "quality-check": "node scripts/pre-commit-check.mjs",
    "quality-gate": "node scripts/quality-gate.mjs",
    "quality-gate:strict": "node scripts/quality-gate.mjs --strict",
    "quality-gate:ci": "node scripts/quality-gate.mjs --strict",
    "pre-commit": "node scripts/pre-commit-check.mjs",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\"",
    "dead-code": "ts-prune -p tsconfig.json",
    "complexity": "echo 'Complexity analysis skipped - tool not available' || echo 'Skipped complexity analysis'",
    "db:health": "node -e \"import('./dist/index.js').then(m => m.databaseManager.healthCheck()).then(healthy => console.log(healthy ? 'âœ… Qdrant DB healthy' : 'âŒ Qdrant DB unhealthy')).catch(console.error)\"",
    "db:generate": "echo 'Database schema generation - Qdrant automatically manages schema'",
    "db:push": "echo 'Database schema push - Qdrant handles schema automatically'",
    "test:connection": "node -e \"import('./dist/index.js').then(m => m.databaseManager.healthCheck()).then(() => console.log('âœ… Qdrant connection OK')).catch(console.error)",
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:coverage": "vitest run --config vitest.coverage.config.ts --coverage",
    "test:coverage:unit": "vitest run --config vitest.config.ts --coverage",
    "test:coverage:integration": "vitest run --config vitest.integration.config.ts --coverage",
    "test:coverage:watch": "vitest --config vitest.coverage.config.ts --coverage --watch",
    "test:coverage:report": "node scripts/generate-coverage-report.js",
    "test:coverage:html": "vitest run --config vitest.coverage.config.ts --coverage --reporter=html",
    "test:coverage:lcov": "vitest run --config vitest.coverage.config.ts --coverage --reporter=lcov",
    "test:coverage:json": "vitest run --config vitest.coverage.config.ts --coverage --reporter=json",
    "test:coverage:summary": "vitest run --config vitest.coverage.config.ts --coverage --reporter=text-summary",
    "test:coverage:ci": "vitest run --config vitest.coverage.config.ts --coverage --reporter=json --reporter=text --reporter=junit",
    "test:coverage:check": "vitest run --config vitest.coverage.config.ts --coverage --run",
    "test:coverage:threshold": "vitest run --config vitest.coverage.config.ts --coverage --coverage.thresholds.autoUpdate=true",
    "test:coverage:merge": "node scripts/merge-coverage-reports.js",
    "test:coverage:upload": "node scripts/upload-coverage-reports.js",
    "test:coverage:badge": "node scripts/generate-coverage-badge.js",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "vitest run --config vitest.ci.config.ts --reporter=verbose --reporter=json --reporter=junit",
    "test:integration:phase6": "node tests/integration/integration-test-runner.mjs",
    "test:integration:happy": "vitest run --config vitest.integration.config.ts tests/integration/qdrant-happy-path.test.ts",
    "test:integration:degraded": "vitest run --config vitest.integration.config.ts tests/integration/qdrant-degraded-path.test.ts",
    "test:integration:reassembly": "vitest run --config vitest.integration.config.ts tests/integration/chunk-reassembly.test.ts",
    "test:integration:performance": "vitest run --config vitest.integration.config.ts tests/integration/performance-smoke.test.ts",
    "test:ci:unit": "vitest run --config vitest.ci.config.ts tests/unit --reporter=verbose --reporter=json",
    "test:ci:coverage": "vitest run --config vitest.ci.config.ts --coverage --reporter=json --reporter=text --reporter=junit",
    "test:ci:validate": "npm run test:ci && npm run verify-test-coverage",
    "test:ci:full": "npm run test:coverage:ci && npm run test:coverage:check",
    "test:profile": "vitest run --config vitest.ci.config.ts --setupFiles ./tests/setup/test-profile-setup.ts",
    "test:profile:validate": "npm run test:profile && npm run verify-test-coverage",
    "test:performance": "vitest run --config vitest.config.ts tests/performance",
    "test:performance:load": "vitest run --config vitest.config.ts tests/performance/load-testing.test.ts",
    "test:performance:memory": "vitest run --config vitest.config.ts tests/performance/memory-profiling.test.ts",
    "test:performance:concurrent": "vitest run --config vitest.config.ts tests/performance/concurrent-users.test.ts",
    "test:performance:stress": "vitest run --config vitest.config.ts tests/performance/stress-testing.test.ts",
    "test:performance:latency": "vitest run --config vitest.config.ts tests/performance/latency-testing.test.ts",
    "test:performance:database": "vitest run --config vitest.config.ts tests/performance/database-performance.test.ts",
    "test:performance:search": "vitest run --config vitest.config.ts tests/performance/search-performance.test.ts",
    "test:performance:all": "npm run test:performance && npm run test:performance:load && npm run test:performance:memory && npm run test:performance:concurrent && npm run test:performance:stress && npm run test:performance:latency && npm run test:performance:database && npm run test:performance:search",
    "test:unit": "vitest run --config vitest.config.ts tests/unit",
    "test:comprehensive": "vitest run --config vitest.config.ts tests/unit/comprehensive-mcp-cortex-tests.test.ts",
    "test:security": "vitest run --config vitest.config.ts tests/security",
    "lint:imports": "eslint src --quiet",
    "verify-test-coverage": "node scripts/verify-test-coverage.js",
    "ops:health": "./scripts/health-check.sh",
    "ops:backup": "./scripts/complete-backup.sh",
    "ops:backup:daily": "npm run ops:backup",
    "ops:backup:manual": "./scripts/manual-backup.sh",
    "ops:backup:selective": "./scripts/selective-backup.sh",
    "ops:backup:verify": "./scripts/verify-backup-integrity.sh",
    "ops:restore": "./scripts/full-restore.sh",
    "ops:restore:test": "./scripts/test-backup-restore.sh",
    "ops:migrate": "./scripts/migrate-environment.sh",
    "ops:emergency": "./scripts/emergency-response.sh",
    "ops:onboard": "./scripts/onboarding-setup.sh",
    "ops:metrics": "./scripts/metrics-collector.sh",
    "ops:baseline": "./scripts/performance-baseline.sh",
    "ops:validate": "./scripts/validate-recovery.sh",
    "ops:status": "systemctl status cortex-mcp qdrant --no-pager && curl -s http://localhost:3000/health | jq .",
    "ops:logs": "tail -f /app/logs/cortex-mcp.log",
    "ops:logs:error": "tail -100 /app/logs/cortex-mcp.log | grep ERROR || echo 'No recent errors found'",
    "ops:restart": "sudo systemctl restart cortex-mcp",
    "docs:build": "npm run build && npm run docs:generate",
    "docs:generate": "node scripts/generate-docs.js",
    "docs:serve": "http-server docs -p 8080 -c-1 --cors",
    "docs:validate": "node scripts/validate-docs.js",
    "docs:index": "node scripts/generate-doc-index.js",
    "docs:search": "node scripts/search-docs.js",
    "docs:new-engineer": "cat docs/NEW-ENGINEER-GUIDE.md",
    "docs:operations": "cat docs/OPS-DISASTER-RECOVERY.md",
    "docs:backup": "cat docs/OPS-BACKUP-MIGRATION.md",
    "docs:api": "cat docs/API-REFERENCE.md",
    "docs:architecture": "cat docs/ARCH-SYSTEM.md",
    "docs:setup": "cat docs/SETUP-QUICK-START.md",
    "docs:all": "npm run docs:generate && npm run docs:validate && npm run docs:index",
    "mcp:check-config": "node scripts/check-mcp-config.js",
    "mcp:validate-tools": "node scripts/validate-mcp-tools.js",
    "mcp:test-tools": "node scripts/test-mcp-tools.js",
    "mcp:debug": "DEBUG=cortex:* node dist/index.js",
    "deploy:dev": "./scripts/deploy.sh development",
    "deploy:staging": "./scripts/deploy.sh staging",
    "deploy:prod": "./scripts/deploy.sh production",
    "deploy:validate": "./scripts/validate-deployment.sh",
    "monitor:setup": "./scripts/setup-monitoring.sh",
    "monitor:dashboard": "./scripts/launch-dashboard.sh",
    "monitor:alerts": "./scripts/setup-alerts.sh",
    "security:audit": "npm audit && npm run lint:security && npm run test:security",
    "security:check": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:scan": "./scripts/security-scan.sh",
    "performance:profile": "node --prof dist/index.js",
    "performance:benchmark": "./scripts/benchmark.js",
    "performance:analyze": "./scripts/analyze-performance.js",
    "system:info": "./scripts/system-info.sh",
    "system:diagnose": "./scripts/diagnose-system.sh",
    "system:cleanup": "./scripts/cleanup-system.sh",
    "quality:full": "npm run type-check && npm run lint && npm run test:coverage && npm run docs:validate",
    "quality:production": "npm run quality-gate:strict && npm run test:ci:full && npm run security:audit && npm run ops:backup:verify",
    "ci:all": "npm run quality:production && npm run docs:all && npm run mcp:validate-tools",
    "dev:full": "npm run build && npm run dev",
    "dev:watch": "npm run build && npm run dev & npm run test:watch",
    "dev:debug": "npm run build && npm run mcp:debug",
    "quickstart": "npm run build && npm run start",
    "help": "echo 'Available scripts:' && npm run | grep -E '^[[:space:]]*[^[:space:]]+' | cut -d: -f1 | sort"
  },
  "keywords": [
    "mcp",
    "memory",
    "knowledge-management",
    "autonomous-decision",
    "qdrant",
    "vector-database",
    "semantic-search",
    "embeddings",
    "disaster-recovery",
    "backup",
    "migration",
    "monitoring",
    "operations",
    "documentation"
  ],
  "author": "Cortex Team",
  "license": "MIT",
  "homepage": "https://github.com/cortex-ai/cortex-memory-mcp#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/cortex-ai/cortex-memory-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/cortex-ai/cortex-memory-mcp/issues"
  },
  "documentation": {
    "main": "README.md",
    "api": "docs/API-REFERENCE.md",
    "operations": "docs/OPS-DISASTER-RECOVERY.md",
    "backup": "docs/OPS-BACKUP-MIGRATION.md",
    "setup": "docs/SETUP-QUICK-START.md",
    "architecture": "docs/ARCH-SYSTEM.md",
    "new-engineer": "docs/NEW-ENGINEER-GUIDE.md",
    "monitoring": "docs/CONFIG-MONITORING.md",
    "deployment": "docs/CONFIG-DEPLOYMENT.md",
    "troubleshooting": "docs/TROUBLESHOOT-ERRORS.md"
  },
  "scripts": {
    "postinstall": "echo 'ðŸ§  Cortex Memory MCP Server v2.0.0 installed successfully!' && echo 'ðŸ“š Quick start: npm run quickstart' && echo 'ðŸ“– Documentation: npm run docs:new-engineer'",
    "preuninstall": "echo 'ðŸ‘‹ Thank you for using Cortex Memory MCP Server!' && echo 'ðŸ’¾ Remember to backup your data before uninstalling'"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@qdrant/js-client-rest": "^1.13.0",
    "@types/helmet": "^0.0.48",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.104.0",
    "pino": "^10.1.0",
    "tsx": "^4.20.6",
    "uuid": "^13.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.52.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.9.1",
    "@types/sinon": "^17.0.4",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "complexity-report": "^2.0.0-alpha",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-security": "^3.0.1",
    "glob": "^11.0.3",
    "prettier": "^3.6.2",
    "sinon": "^21.0.0",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsc-esm-fix": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^4.0.3",
    "vitest-tsconfig-paths": "^3.4.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
